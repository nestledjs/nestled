{
  "$schema": "http://json-schema.org/schema",
  "$id": "ApiLibAccount",
  "title": "Create an API Library",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Library name",
      "default": "account"
    },
    "directory": {
      "type": "string",
      "description": "Directory where the lib is placed",
      "default": "libs/api/account"
    }
  },
  "required": ["directory"]
}
