{
  "generators": {
    "api-libs": {
      "factory": "./src/api-libs/generator",
      "schema": "./src/api-libs/schema.json",
      "description": "Generates Default API Libraries"
    },
    "api-gen": {
      "factory": "./src/api-gen/generator",
      "schema": "./src/api-gen/schema.json",
      "description": "Runs All API Generators"
    },
    "project-config": {
      "factory": "./src/project-config/generator",
      "schema": "./src/project-config/schema.json",
      "description": "Generates Top Level Project Config"
    },
    "api-app": {
      "factory": "./src/api-app/generator",
      "schema": "./src/api-app/schema.json",
      "description": "Generates Basic Nest API App"
    },
    "remix-app": {
      "factory": "./src/remix-app/generator",
      "schema": "./src/remix-app/schema.json",
      "description": "Generates Basic Remix App"
    },
    "api-crud": {
      "factory": "./src/api-crud/generator",
      "schema": "./src/api-crud/schema.json",
      "description": "Generates API CRUD operations"
    },
    "crud": {
      "factory": "./src/crud/generator",
      "schema": "./src/crud/schema.json",
      "description": "Generates Full CRUD Operations"
    },
    "shared": {
      "factory": "./src/shared/generator",
      "schema": "./src/shared/schema.json",
      "description": "Generates Shared Utilities and Modules"
    },
    "web-admin-crud": {
      "factory": "./src/web-admin-crud/generator",
      "schema": "./src/web-admin-crud/schema.json",
      "description": "Generates Admin CRUD Operations for Web"
    },
    "web-admin-crud-helper": {
      "factory": "./src/web-admin-crud-helper/generator",
      "schema": "./src/web-admin-crud-helper/schema.json",
      "description": "Generates Helper Functions for Admin CRUD"
    },
    "web-component": {
      "factory": "./src/web-component/generator",
      "schema": "./src/web-component/schema.json",
      "description": "Generates Web Components"
    },
    "web-feature": {
      "factory": "./src/web-feature/generator",
      "schema": "./src/web-feature/schema.json",
      "description": "Generates Feature Modules for Web"
    },
    "web-module": {
      "factory": "./src/web-module/generator",
      "schema": "./src/web-module/schema.json",
      "description": "Generates Web Module"
    },
    "web-user-crud": {
      "factory": "./src/web-user-crud/generator",
      "schema": "./src/web-user-crud/schema.json",
      "description": "Generates User CRUD Operations for Web"
    },
    "workspace-setup": {
      "factory": "./src/workspace-setup/generator",
      "schema": "./src/workspace-setup/schema.json",
      "description": "Generates Workspace Setup"
    },
    "api-dependencies": {
      "factory": "./src/api-dependencies/generator",
      "schema": "./src/api-dependencies/schema.json",
      "description": "Sets up all dependencies for API App"
    }
  }
}
